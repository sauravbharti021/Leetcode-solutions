class Solution {
public:
    int canBeTypedWords(string text, string brokenLetters) {
        int n= text.size();
        int cnt = 0;
        for(int i=0;i<n;i++){
            string x = "";
            int j=i;
            int flag = 0;
            while(j<n && text[j]!=' '){
                x+= text[j];
                if(brokenLetters.find(text[j])!=-1){
                    flag = 1;
                }
                j+=1;
            }
            if(!flag) cnt+=1;
            i=j;
        }
        return cnt;
    }
};
