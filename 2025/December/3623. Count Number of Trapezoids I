#define mod 1000000007
#define ll long long
class Solution {
public:
    int countTrapezoids(vector<vector<int>>& points) {
        unordered_map<ll,ll> m;
        for(auto i: points){
            m[i[1]]+=1;
        }

        vector<ll> fre;
        for(auto i: m){
            fre.push_back(((i.second-1)*(i.second))/2);
        }
        int n = fre.size();
        vector<ll> pre(n, 0);
        for(int i=n-1;i>=0;i--){
            pre[i]= (pre[i] + fre[i] )%mod;
            if(i+1<n){
                pre[i] = (pre[i] + pre[i+1])%mod;
            }
        }
        ll ans = 0;
        for(int i=0;i<n-1;i++){
            ans= (ans + (fre[i]*pre[i+1])%mod)%mod;
        }
        ans%=mod;
        if(ans<0) ans+=mod;
        return ans;
    }
};
