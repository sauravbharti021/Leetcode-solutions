class Solution {
public:
    int finalValueAfterOperations(vector<string>& operations) {
        int curValue = 0;
        int n = operations.size();
        for(int i=0;i<n;i++){
            if(operations[i][0]=='+' || operations[i].back()=='+') curValue+=1;
            else curValue -=1;
        }

        return curValue;

    }
};
