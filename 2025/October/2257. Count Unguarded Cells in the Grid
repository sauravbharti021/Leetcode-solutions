class Solution {
public:
    int countUnguarded(int m, int n, vector<vector<int>>& guards, vector<vector<int>>& walls) {
        vector<vector<int>> guardsPresent(m, vector<int>(n, 0)), wallsPresent(m, vector<int>(n, 0)); 
        for(int i=0;i<guards.size();i++){
            guardsPresent[guards[i][0]][guards[i][1]] = 1;
        }
        for(int i=0;i<walls.size();i++){
            wallsPresent[walls[i][0]][walls[i][1]] = 1;
        }

        vector<vector<bool>> pre(m, vector<bool>(n, false));
        for(int i=0;i<m;i++){
            bool prev = false;
            for(int j=0;j<n;j++){

                pre[i][j] = pre[i][j] | prev;


                if(wallsPresent[i][j]){
                    prev = false;
                }
                if(guardsPresent[i][j]){
                    prev = true;
                }
            }
        }
        for(int i=m-1;i>=0;i--){
            bool prev = false;
            for(int j=n-1;j>=0;j--){

                pre[i][j] = pre[i][j] | prev;


                if(wallsPresent[i][j]){
                    prev = false;
                }
                if(guardsPresent[i][j]){
                    prev = true;
                }
            }
        }
        for(int i=0;i<n;i++){
            bool prev = false;
            for(int j=0;j<m;j++){

                pre[j][i] = pre[j][i] | prev;

                if(wallsPresent[j][i]){
                    prev = false;
                }
                if(guardsPresent[j][i]){
                    prev = true;
                }
            }
        }
        for(int i=n-1;i>=0;i--){
            bool prev = false;
            for(int j=m-1;j>=0;j--){

                pre[j][i] = pre[j][i] | prev;

                if(wallsPresent[j][i]){
                    prev = false;
                }
                if(guardsPresent[j][i]){
                    prev = true;
                }
            }
        }

        int ans = 0;
        for(int i=0;i<m;i++){
            for(int j=0;j<n;j++){
                if(!wallsPresent[i][j] && !guardsPresent[i][j]){
                    ans += !pre[i][j];
                }
            }
        }

        return ans;
    }
};
