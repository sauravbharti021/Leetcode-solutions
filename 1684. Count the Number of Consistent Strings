class Solution {
public:
    int countConsistentStrings(string allowed, vector<string>& words) {
        set<char> st;
        for(int i=0;i<allowed.size();i++){
            st.insert(allowed[i]);
        }
        int cnt = 0;
        for(int i=0;i<words.size();i++){
            int flag = 0;
            for(int j=0;j<words[i].size();j++){
                if(st.find(words[i][j])==st.end()){
                    flag=1;
                    break;
                }
            }
            if(!flag) cnt+=1;
        }
        
        return cnt;
    }
};
